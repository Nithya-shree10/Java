{
    Scanner s = new Scanner(System.in);
    int[] score = new int [5];
    score[0]=s.nextInt();
     score[1]=s.nextInt();
      score[2]=s.nextInt();
       score[3]=s.nextInt();
        score[4]=s.nextInt();
